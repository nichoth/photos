{
  "name": "@nichoth/photos",
  "description": "The UI for a photo viewing thing. Does not include a backend/protocol.",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-sass && concurrently --kill-others \"npm run serve\" \"npm run watch-sass\"",
    "serve": "budo src/index.js:bundle.js --dir=public --open --live -- -p esmify -t uglifyify -dv",
    "build-sass": "sass src/style/style.scss public/style.css",
    "watch-sass": "sass --watch src/style/style.scss public/style.css",
    "build": "browserify --debug -t uglifyify -p esmify src/index.js > public/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/photos.git"
  },
  "keywords": [
    "photos",
    "image",
    "pictures"
  ],
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/photos/issues"
  },
  "homepage": "https://github.com/nichoth/photos#readme",
  "dependencies": {
    "@nichoth/events": "^1.1.2",
    "htm": "^3.0.4",
    "observ": "^0.2.0",
    "observ-struct": "^6.0.0",
    "preact": "^10.5.12",
    "route-event": "^3.1.0",
    "ruta3": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "budo": "^11.6.4",
    "concurrently": "^6.0.0",
    "esmify": "^2.1.1",
    "sass": "^1.32.8",
    "uglifyify": "^5.0.2"
  }
}
